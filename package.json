{
  "name": "@technote-space/gutenberg-utils",
  "version": "2.2.4",
  "description": "gutenberg utils",
  "author": {
    "name": "Technote",
    "email": "technote.space@gmail.com",
    "url": "https://technote.space"
  },
  "license": "GPL-3.0",
  "keywords": [
    "gutenberg"
  ],
  "homepage": "https://github.com/technote-space/gutenberg-utils#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/technote-space/gutenberg-utils.git"
  },
  "bugs": {
    "url": "https://github.com/technote-space/gutenberg-utils/issues"
  },
  "main": "build/index.js",
  "files": [
    "build"
  ],
  "dependencies": {
    "classnames": "^2.2.6",
    "nano-css": "^5.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.3",
    "@babel/core": "^7.10.3",
    "@babel/plugin-transform-react-jsx": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@technote-space/gutenberg-test-helper": "^0.0.16",
    "@wordpress/block-editor": "^4.2.0",
    "@wordpress/components": "^9.9.0",
    "@wordpress/data": "^4.21.0",
    "@wordpress/element": "^2.15.0",
    "@wordpress/i18n": "^3.14.0",
    "@wordpress/rich-text": "^3.19.0",
    "@wordpress/url": "^2.17.0",
    "babel-jest": "^26.1.0",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.5.0",
    "eslint": "^7.3.1",
    "eslint-plugin-react": "^7.20.2",
    "jest": "^26.1.0",
    "react": "^16.13.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "yarn build",
    "test": "yarn lint && yarn cover",
    "test:update": "yarn lint && yarn cover:update",
    "lint": "eslint 'src/**/*.js' '__tests__/**/*.js' --cache",
    "lint:fix": "eslint --fix 'src/**/*.js' '__tests__/**/*.js'",
    "cover": "jest --coverage",
    "cover:update": "jest --coverage --updateSnapshot",
    "develop": "rm -rdf build && babel src --out-dir build",
    "build": "rm -rdf build && babel --compact=true src --out-dir build",
    "update": "npx npm-check-updates -u && yarn install && yarn upgrade && yarn audit"
  }
}
