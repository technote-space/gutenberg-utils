{
  "name": "@technote-space/gutenberg-utils",
  "version": "2.1.7",
  "description": "gutenberg utils",
  "author": {
    "name": "Technote",
    "email": "technote.space@gmail.com",
    "url": "https://technote.space"
  },
  "license": "GPL-3.0",
  "keywords": [
    "gutenberg"
  ],
  "homepage": "https://github.com/technote-space/gutenberg-utils#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/technote-space/gutenberg-utils.git"
  },
  "bugs": {
    "url": "https://github.com/technote-space/gutenberg-utils/issues"
  },
  "main": "build/index.js",
  "files": [
    "build"
  ],
  "dependencies": {
    "classnames": "^2.2.6",
    "nano-css": "^5.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-react-jsx": "^7.9.4",
    "@babel/preset-env": "^7.9.5",
    "@technote-space/gutenberg-test-helper": "^0.0.12",
    "@wordpress/block-editor": "^3.9.1",
    "@wordpress/components": "^9.4.1",
    "@wordpress/data": "^4.16.1",
    "@wordpress/element": "^2.13.1",
    "@wordpress/i18n": "^3.11.0",
    "@wordpress/rich-text": "^3.14.1",
    "@wordpress/url": "^2.13.0",
    "babel-jest": "^25.3.0",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.4.4",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.19.0",
    "jest": "^25.3.0",
    "react": "^16.13.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "yarn build",
    "test": "yarn lint && yarn cover",
    "test:update": "yarn lint && yarn cover:update",
    "lint": "eslint 'src/**/*.js' '__tests__/**/*.js' --cache",
    "lint:fix": "eslint --fix 'src/**/*.js' '__tests__/**/*.js'",
    "cover": "jest --coverage",
    "cover:update": "jest --coverage --updateSnapshot",
    "develop": "rm -rdf build && babel src --out-dir build",
    "build": "rm -rdf build && babel --compact=true src --out-dir build",
    "update": "ncu -u && yarn install && yarn upgrade && yarn audit"
  }
}
